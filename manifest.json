{
	"manifest_version": 2,
	"name": "BookmarkTitler",
	"version": "1.0",
	"description": "Sets bookmark tab titles to the name of the bookmarks",
	"icons": {
		"48": "icon.svg",
		"96": "icon.svg"
	},
	"permissions": [
		"*://*/*",
		"bookmarks",
		"storage",
		"tabs"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"options_ui": {
		"page": "options.html"
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "{225d6e43-640a-4630-801f-cf12a6e8501b}"
		}
	}
}

